{
  "$schema": "https://static.modelcontextprotocol.io/schemas/2025-10-17/server.schema.json",
  "name": "io.github.cmd8/excalidraw-mcp",
  "description": "Model Context Protocol server for Excalidraw diagrams.",
  "repository": {
    "url": "https://github.com/cmd8/excalidraw-mcp",
    "source": "github"
  },
  "version": "1.0.0",
  "packages": [
    {
      "registryType": "npm",
      "registryBaseUrl": "https://registry.npmjs.org",
      "identifier": "@cmd8/excalidraw-mcp",
      "version": "1.0.0",
      "transport": {
        "type": "stdio"
      },
      "environmentVariables": [],
      "packageArguments": [
        {
          "name": "diagram",
          "description": "Path to the Excalidraw diagram file",
          "isRequired": true,
          "valueHint": "/path/to/diagram.excalidraw"
        }
      ]
    }
  ]
}
